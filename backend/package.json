{
  "name": "truesocial-backend",
  "version": "1.0.0",
  "description": "Backend API pour TrueSocial - Instagram clone",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "start:check": "powershell -ExecutionPolicy Bypass -File ./scripts/start.ps1",
    "build": "prisma generate",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "db:setup": "powershell -ExecutionPolicy Bypass -File ./scripts/setup-db.ps1",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "setup": "npm install && npm run prisma:generate",
    "setup:full": "npm run setup && npm run db:setup && npm run prisma:migrate && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "prisma": "^6.16.3",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "instagram",
    "social-media",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "redis",
    "websocket"
  ],
  "author": "TrueSocial Team",
  "license": "MIT"
}
